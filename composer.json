{
  "name": "jcbowen/easywechat-yii2",
  "description": "WeChat SDK for Yii2 , 基于 w7corp/easywechat 5.x",
  "keywords": [
    "yii",
    "yii2",
    "wechat",
    "easywechat",
    "php",
    "jcbowen"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Bowen",
      "email": "bowen@jiuchet.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "^2.0.4",
    "w7corp/easywechat": "^5.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Jcbowen\\EasyWechatYii2\\": "./src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "easywechat-composer/easywechat-composer": true,
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
